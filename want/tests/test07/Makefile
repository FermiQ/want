MAKE=make

info:
	@echo " "
	@echo " Possible targets are: "
	@echo "     all           perform all the calculation steps"
	@echo "     want          perform only the WanT calculations"
	@echo "     clean         clean up"
	@echo " "

all: 
	./run_CoUS.sh all
	./run_AuUS.sh all
	./run_Au.sh all

want: 
	./run_CoUS.sh want
	./run_AuUS.sh want
	./run_Au.sh want

clean:
	./run_CoUS.sh clean
	./run_AuUS.sh clean
	./run_Au.sh clean
