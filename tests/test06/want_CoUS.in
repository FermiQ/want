
&CONTROL
   prefix = 'cobalt_US'
   postfix = '_WanT'
   work_dir = './'
   verbosity = 'high'
/

&SUBSPACE
   dimwann = 6   
/

&LOCALIZATION
   maxiter0_wan = 100
   maxiter1_wan = 1500
   ncg = 5    
   nshells = 2  
   nwhich(1) = 1  
   nwhich(2) = 2  
/

